// Coding Exercise 8 - Teen Number Checker
public class TeenNumberChecker {
    // write code here
    public static boolean hasTeen(int num1, int num2, int num3)
    {
        if(isTeen(num1) || isTeen(num2) || isTeen(num3))
        {
            return true;
        }
        return false;
    }
    
    public static boolean isTeen(int num1)
    {
        if(num1 >= 13 && num1 <= 19)
        {
            return true;
        }
        return false;
    }
}
